        "verses": [
            {
                "number": "1",
                "lines": [
                    "1. Isaorana anie Andriamanitsika",
                    "Fa fitahiam-be Omeny ho antsika",
                    "Hatreo ambohoka",
                    "Ka mandraka antitra,",
                    "Isika henika",
                    "Ny soan'ny lanitra"
                ]
            },
